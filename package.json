{
  "name": "linczero-vuepress",
  "version": "1.0.0",
  "description": "Linc's WebSite",
  "license": "GPLv3",
  "type": "module",
  "scripts": {
    "docs:build": "pnpm check-mem && vuepress-vite build src",
    "docs:clean-dev": "vuepress-vite dev src --clean-cache",
    "docs:dev": "vuepress-vite dev src",
    "docs:update-package": "pnpm dlx vp-update",
    "check-mem": "node --max-old-space-size=10240 -e 'console.log(v8.getHeapStatistics())'"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.8",
    "@vuepress/plugin-docsearch": "2.0.0-rc.21",
    "@vuepress/utils": "2.0.0-rc.2",
    "@vueuse/core": "^10.9.0",
    "jsdom": "^24.1.1",
    "markdown-it-container": "^4.0.0",
    "markmap-lib": "^0.17.0",
    "markmap-toolbar": "^0.17.0",
    "markmap-view": "^0.17.0",
    "plantuml-encoder": "^1.4.0",
    "vue": "^3.4.21",
    "vuepress": "2.0.0-rc.8",
    "vuepress-theme-hope": "2.0.0-rc.44"
  },
  "dependencies": {
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@vuepress/helper": "2.0.0-rc.19",
    "katex": "^0.16.9",
    "mermaid": "^10.9.1"
  }
